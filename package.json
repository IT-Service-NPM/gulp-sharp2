{
	"name": "gulp-sharp2",
	"type": "module",
	"version": "1.1.2",
	"description": "A Gulp plugin for images processing with Sharp",
	"keywords": [
		"gulpplugin",
		"gulp",
		"images",
		"sharp"
	],
	"author": {
		"name": "Sergei S. Betke",
		"email": "Sergey.S.Betke@yandex.ru",
		"url": "https://github.com/sergey-s-betke"
	},
	"homepage": "https://github.com/IT-Service-NPM/gulp-sharp2",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/IT-Service-NPM/gulp-sharp2.git"
	},
	"bugs": {
		"url": "https://github.com/IT-Service-NPM/gulp-sharp2/issues"
	},
	"license": "MIT",
	"engines": {
		"node": ">=22.0"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./gulp-sharp": {
			"import": {
				"types": "./dist/gulp-sharp/index.d.ts",
				"default": "./dist/gulp-sharp/index.js"
			},
			"require": {
				"types": "./dist/gulp-sharp/index.d.cts",
				"default": "./dist/gulp-sharp/index.cjs"
			}
		}
	},
	"imports": {
		"#gulp-sharp2": "./src/index.ts",
		"#gulp-sharp2/gulp-sharp": "./src/gulp-sharp/index.ts"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup-node",
		"CI": "npm run build && npm run lint && npm run test && npm run update-readme-API && npm run update-readme",
		"check-exports": "attw --pack .",
		"lint": "tsc && eslint",
		"test": "vitest run --coverage",
		"update-readme-API": "npx api-extractor run --local && npx api-documenter markdown -i ./temp -o ./docs",
		"update-examples-readme": "remark test/examples/*/readme.md --output",
		"update-main-readme": "remark .readme.md --output README.md",
		"update-readme": "npm run update-examples-readme && npm run update-main-readme",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
	},
	"dependencies": {
		"gulplog": "^2.2.0",
		"plugin-error": "^2.0.1",
		"sharp": "^0.34.5",
		"streamx": "^2.23.0",
		"vinyl": "^3.0.1"
	},
	"devDependencies": {
		"@altano/vitest-plugins": "^1.1.1",
		"@commitlint/cli": "^20.4.1",
		"@commitlint/config-angular": "^20.4.1",
		"@commitlint/config-conventional": "^20.4.1",
		"@commitlint/types": "^20.4.0",
		"@eslint/js": "^9.13.0",
		"@it-service-npm/remark-include": "^4.2.0",
		"@microsoft/api-documenter": "^7.29.0",
		"@microsoft/api-extractor": "^7.57.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^12.0.6",
		"@semantic-release/npm": "^13.1.4",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"@szum-tech/semantic-release-config": "^2.3.7",
		"@types/gulp": "^4.0.18",
		"@types/gulp-rename": "^2.0.7",
		"@types/node": "^25.3.0",
		"@typescript-eslint/eslint-plugin": "^8.12.2",
		"@typescript-eslint/parser": "^8.12.2",
		"@vitest/coverage-istanbul": "^4.0.18",
		"@vitest/eslint-plugin": "^1.6.9",
		"conventional-changelog-cli": "^4.1.0",
		"eslint": "^9.39.2",
		"eslint-plugin-tsdoc": "^0.5.0",
		"gulp": "^5.0.1",
		"gulp-cli": "^3.1.0",
		"gulp-rename": "^2.1.0",
		"gulp-sharp2": "file:",
		"looks-same": "^10.0.1",
		"remark-alerts": "^0.0.1",
		"remark-cli": "^12.0.1",
		"remark-code-import": "^1.2.0",
		"remark-gfm": "^4.0.1",
		"remark-github-blockquote-alert": "^2.0.1",
		"remark-license": "^6.1.0",
		"remark-lint-code-block-style": "^4.0.1",
		"remark-preset-lint-consistent": "^6.0.1",
		"remark-preset-lint-recommended": "^7.0.1",
		"remark-toc": "^9.0.0",
		"remark-typography": "^0.7.3",
		"remark-validate-links": "^13.1.0",
		"semantic-release": "^25.0.3",
		"tsdoc-markdown": "^1.4.1",
		"tsup": "^8.5.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0",
		"vite-plugin-vitest-typescript-assert": "^1.1.4",
		"vitest": "^4.0.18"
	}
}
